#!/usr/bin/env bash
case "$1" in
  stop)
    echo "testid"
    ;;
  start)
    echo "testid"
    ;;
  kill)
    echo "testid"
    ;;
  run)
    echo "testid"
    ;;
  rm)
    echo "testid"
    ;;
  ps)
    if [[ $@ = *"no-trunc"* ]]; then
      echo "1479bbd60ade8a92617d2aeb4935bd3ff3179bd0fd71c22c3102c421f4bc221f"
      exit 0
    else
      echo "CONTAINER ID        IMAGE                      COMMAND                CREATED             STATUS                         PORTS               NAMES"
      echo "mariadb             mariadb:10.0.21            \"/docker-entrypoint.   10 seconds ago      Up 10 seconds                  3306/tcp            dokku.mariadb.l"
    fi
    ;;
  exec)
    echo "exec called with $@"
    ;;
  inspect)
    if [[ $@ = *"IPAddress"* ]]; then
      echo "172.17.0.34"
      exit 0
    fi

    # running
    echo "true"
    ;;
  images)
    echo "REPOSITORY                TAG                 IMAGE ID            CREATED             VIRTUAL SIZE"
    echo "mariadb                   10.0.21             f2485761e714        11 days ago         302.2 MB"
    ;;
  pull)
    exit 0
    ;;
  *)
    exit "$DOKKU_NOT_IMPLEMENTED_EXIT"
    ;;
esac
